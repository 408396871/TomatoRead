{
  "manifest_version": 2,

  "name": "LinkShare",
  "description": "Save a link, or share a link. Build your internet links.",
  "version": "0.1",

  "browser_action": {
    "default_icon": {
    	"38":"image/icon.png"
	},
    "default_popup": "popup.html",
    "default_title" : "Save or Share this link"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "https://ajax.googleapis.com/",
    "http://127.0.0.1/",
    "http://127.0.0.1:8086/",
    "http://127.0.0.1:8080/"
  ]
}