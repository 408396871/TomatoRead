{
  "manifest_version": 2,

  "name": "LinkHubChrome",
  "description": "Build your internet links.",
  "version": "0.2",

  "browser_action": {
    "default_icon": {
    	"38":"image/icon.png"
	},
    "default_popup": "popup.html",
    "default_title" : "Save or Share links"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "http://linkhub.pub/",
    "http://linkhub.app:8080/"
  ]
}