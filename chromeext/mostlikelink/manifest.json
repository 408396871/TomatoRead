{
  "manifest_version": 2,

  "name": "mostlike.link",
  "description": "your most like links",
  "version": "0.3",
  "background": {
    "scripts": [
      "vendor/jquery-2.1.4.min.js",
      "js/common.js",
      "js/background.js"
    ]
  },
  "page_action": {
    "default_icon": "image/off.png",
    "default_popup" : "app.html",
    "default_title" : "Like"
  },
  "permissions": [
    "tabs",
    "http://mostlike.link/",
    "http://0.0.0.0:5000/"
  ],
  "icons":{
    "48":"image/on.png",
    "128":"image/on.png"
  }
}